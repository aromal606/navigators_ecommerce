                            <li class="dropdown-item"  onclick="yearlyReport()">This Year</li>
                        </ul>
                        </div>

                    <div class="card-body">
                  <h5 class="card-title">Reports <span>/Today</span></h5>




                  <!-- Line Chart -->
                  <canvas id="line-chart"></canvas>

                 
                    <script>
                              var myChart;
                              var myChart0;
                              var myChart3
                              var chart1
                              var chart0
                              var chart3

                            window.onload = () => {
                            dailyReport()
                            dayReport()
                            daily_Report()
                            overAllReport()
                            };


                              
                             function dailyReport(){
                                   
                              chart1 = true;


                            const data = {
                                labels: [
                                  {{#each dailySale}}
                                  '{{_id}}',
                                  {{/each}}
                                ],
                                datasets: [{
                                  label: 'daily sales',
                                  data: [{{#each dailySale}}'{{totalAmount}}',{{/each}}],
                                  backgroundColor: [
                                    'rgb(103 119 239)',
                                    'rgb(102 187 106)',
                                    'rgb(54, 162, 235)',
                                    'rgb(255, 99, 132)'
                                  ]
                                }]
                              };
                              
                              const config = {
                                type: 'bar',
                                data: data,
                                options: {
                                  aspectRatio:3,
                                  responsive:true
                                } 
                              }; 
                              if(chart0){
                                myChart0.destroy()
                              }
                              if(chart3){
                                myChart3.destroy()
                              }         
                                        
                            myChart = new Chart(document.getElementById('line-chart'), config)

                                                  
                            } 

                            




                            function yearlyReport(){


                              chart3 = true;


                            const data = {
                                labels: [
                                  {{#each yearlySale}}'{{_id}}',{{/each}}
                                ],
                                datasets: [{
                                  label: 'Yearly Sales',
                                  data: [{{#each yearlySale}}'{{totalAmount}}',{{/each}}],
                                  backgroundColor: [
                                    'rgb(103 119 239)',
                                    'rgb(102 187 106)',
                                    'rgb(54, 162, 235)',
                                    'rgb(255, 99, 132)'
                                  ]
                                }]
                              };
                              
                              const config = {
                                type: 'pie',
                                data: data,
                                options: {
                                  aspectRatio:3,
                                  responsive:true
                                }
                              }; 
                            if(chart0){
                                myChart0.destroy()
                              }
                              if(chart1){
                                myChart.destroy()
                              }  
                            myChart3 = new Chart(document.getElementById('line-chart'), config)

                                                  
                            }



                             function monthReport(){
                                   
                              chart0 = true;


                            const data = {
                                labels: [
                                  {{#each monthSales}}'{{_id}}',{{/each}}
                                ],
                                datasets: [{
                                  label: 'monthly sales',
                                  data: [{{#each monthSales}}'{{totalAmount}}',{{/each}}],
                                  backgroundColor: [
                                    'rgb(103 119 239)',
                                    'rgb(102 187 106)',
                                    'rgb(54, 162, 235)',
                                    'rgb(255, 99, 132)'
                                  ]
                                }]
                              };
                              
                              const config = {
                                type: 'pie',
                                data: data,
                                options: {
                                  aspectRatio:3,
                                  responsive:true
                                } 
                              }; 
                              if(chart1){
                                myChart.destroy()
                              }
                              if(chart3){
                                myChart3.destroy()
                              }         
                                        
                            myChart0 = new Chart(document.getElementById('line-chart'), config)

                                                  
                            } 




                        </script>
                  <!-- End Line Chart -->

                </div>

              </div>
          
          
