<style>
    .modal-content {
        background-color: #ffffff;
        border-color: #0a0a0a;

    }



    .show {
        padding: 0;
    }

    .modal-header {
        border-bottom: none;
        text-align: center;
    }

    .modal-header .close {

        color: #fff;
    }

    :-moz-any-link:focus {
        outline: none;
    }

    .modal-title {}

    .modal-body {
        padding: 1rem;
    }

    #progressbar {
        margin-bottom: 3vh;
        overflow: hidden;
        color: rgb(8, 8, 8);
        padding-left: 0px;
        margin-top: 3vh
    }

    #progressbar li {
        list-style-type: none;
        font-size: 0.8rem;
        width: 25%;
        float: left;
        position: relative;
        font-weight: 400;
        color: rgb(7, 7, 7);
    }

    #progressbar #step1:before {
        content: "";
        color: rgb(7, 7, 7);
        width: 20px;
        height: 20px;
        margin-left: 0px !important;
    }

    #progressbar #step2:before {
        content: "";
        color: rgb(5, 5, 5);
        width: 20px;
        height: 20px;
        margin-left: 32%;
    }

    #progressbar #step3:before {
        content: "";
        color: rgb(12, 12, 12);
        width: 20px;
        height: 20px;
        margin-right: 32%;
    }

    #progressbar #step4:before {
        content: "";
        color: rgb(151, 149, 149, 0.651);
        width: 20px;
        height: 20px;
        margin-right: 0px !important;
    }

    #progressbar li:before {
        line-height: 29px;
        display: block;
        font-size: 12px;
        background: rgb(151, 149, 149);
        border-radius: 50%;
        margin: auto;
        z-index: -1;
        margin-bottom: 1vh;
    }

    #progressbar li:after {
        content: '';
        height: 3px;
        background: rgb(151, 149, 149, 0.651);
        position: absolute;
        left: 0%;
        right: 0%;
        margin-bottom: 2vh;
        top: 8px;
        z-index: 1;
    }

    .progress-track {
        padding: 0 8%;
    }

    #progressbar li:nth-child(2):after {
        margin-right: auto;
    }

    #progressbar li:nth-child(1):after {
        margin: auto;
    }

    #progressbar li:nth-child(3):after {
        float: left;
        width: 68%;
    }

    #progressbar li:nth-child(4):after {
        margin-left: auto;
        width: 132%;
    }

    #progressbar li.active:before,
    #progressbar li.active:after {
        background: rgb(10, 10, 10);
    }

    #three {
        font-size: 1.2rem;
    }

    @media (max-width: 767px) {
        #three {
            font-size: 1rem;
        }
    }

    .details {
        padding: 2rem;
        font-size: 15px;
        line-height: 3.5rem;
    }

    @media (max-width: 767px) {
        .details {
            padding: 2rem 0;
            font-size: 15px;
            line-height: 2.5rem;
        }
    }

    .d-table {
        width: 100%;
    }

    .d-table-row {
        width: 100%;
    }

    .d-table-cell {
        padding-left: 3rem;
    }

    @media (max-width: 767px) {
        .d-table-cell {
            padding-left: 1rem;
        }
    }

    .col-3 {
        display: grid;
        text-align: end;
    }

    .col-3 .d-table-row {
        align-self: flex-end;
    }

    .fa {
        font-size: xx-large;
        text-align: center;
        width: 3rem;
        padding: 0.5rem;
        color: #42469d;
        background-color: rgb(14, 13, 13);
        border-radius: 2rem;
        bottom: 0;
        right: 0;
    }
</style>


