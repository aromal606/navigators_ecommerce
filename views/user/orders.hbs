<div id="page-content">
    <!--Page Title-->
    <div class="page section-header text-center">
        <div class="page-title">
            <div class="wrapper">
                <h1 class="page-width">My Order</h1>


            </div>
        </div>
    </div>
    <!--End Page Title-->

    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-12 col-md-12 col-lg-12 main-col">
                <form action="#">
                    <div class="wishlist-table table-content table-responsive" id="btnRefresh">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th class="product-name text-center alt-font">No</th>
                                    <th class="product-price text-center alt-font">ordered date</th>
                                    <th class="product-name alt-font">address</th>
                                    <th class="product-price text-center alt-font">amount</th>
                                    <th class="stock-status text-center alt-font">payment</th>
                                    <th class="product-subtotal text-center alt-font">status</th>
                                    <th class="product-subtotal text-center alt-font"></th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each orders}}

                                <tr>
                                    <td class="product-name">
                                        <h4 class="no-margin"><a href="#">{{inc @index}}</a></h4>
                                    </td>
                                    <td class="product-name">
                                        <h4 class="no-margin"><a href="#">{{this.orderDetails.date}}</a></h4>
                                    </td>
                                    <td class="product-name">
                                        <h4 class="no-margin"><a
                                                href="#">{{this.orderDetails.deliveryDetails.name}},{{this.orderDetails.deliveryDetails.address}}<br>city:
                                                {{this.orderDetails.deliveryDetails.city}},
                                                {{this.orderDetails.deliveryDetails.state}}<br>pin:
                                                {{this.orderDetails.deliveryDetails.pincode}}<br>phone:
                                                {{this.orderDetails.deliveryDetails.phone}}</a></h4>
                                    </td>
                                    <td class="product-price text-center"><span
                                            class="amount">{{this.orderDetails.totalAmount}}</span></td>
                                    <td class="product-name">
                                        <h4 class="no-margin"><a href="#">{{this.orderDetails.paymentMethod}}</a></h4>
                                    </td>

                                    <td class="stock text-center">
                                        {{#ifCond this.orderDetails.status "return request"}}
                                        <span class="in-stock">return requst pending for validation</span>
                                        {{else}}
                                        <span class="in-stock">{{this.orderDetails.status}}</span><br>
                                        {{/ifCond}}
                                        {{#ifCond this.orderDetails.refund "complete"}}
                                        <span class="in-stock text-success">refund success</span>
                                        {{else}}

                                        {{/ifCond}}

                                    </td>
                                    <td>


                                        <!-- Button trigger modal -->
                                        <button type="button" class="btn btn-primary" data-toggle="modal"
                                            onclick="orderProductView('{{this._id}}')"
                                            data-target=".bd-example-modal-lg{{this._id}}">Details</button>

                                        <!-- Modal -->
                                        <div class="modal fade bd-example-modal-lg{{this._id}}" tabindex="-1"
                                            role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
                                            <div class="modal-dialog modal-lg modal-dialog-scrollable">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title" id="exampleModalLabel">detail view</h5>
                                                        <br>

                                                        <button type="button" class="close" data-dismiss="modal"
                                                            aria-label="Close">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                    </div>
                                                    <div class="modal-body">
                                                        order
                                                            ID:{{this.orderDetails._id}}</h4>
                                                        <h4> placed on:{{this.orderDetails.date}}</h4>
                                                        <table class="table">
                                                            <thead>
                                                                <tr>
                                                                    <th scope="col">no</th>
                                                                    <th scope="col">product</th>
                                                                    <th scope="col">name</th>
                                                                    <th scope="col">price</th>
                                                                    <th scope="col">Quantity</th>
                                                                </tr>
                                                            </thead>
                                                            {{#each this.productList}}
                                                            <tbody>
                                                                <tr>
                                                                    <th scope="row">{{inc @index}}</th>
                                                                    <td class="cart__image-wrapper cart-flex-item">
                                                                        <a href="#"><img class="cart__image"
                                                                                style="width: 65px;"
                                                                                src="/pro_img/{{this.product.img.[0]}}"
                                                                                alt=""></a>
                                                                    </td>
                                                                    <td>{{this.product.productname}}</td>
                                                                    <td>{{this.product.price}}</td>
                                                                    <td>{{this.quantity}}</td>
                                                                </tr>

                                                            </tbody>
                                                            {{/each}}
                                                        </table>
                                                        <!-- Button to Open the Modal -->
                                                            <div>
                                                           <h4 style="font-weight: bold;">Track Your Order</h4>
                                                            </div>
                                                        <div class="progress-track">
                                                            <ul id="progressbar">
                                                                {{!-- {{#each orders}} --}}
                                                                {{#ifCond this.orderDetails.status "placed"}}
                                                                <li class="step0 active" id="step1">Order placed</li>
                                                                <li class="step0  text-center" id="step2">shipped</li>
                                                                <li class="step0  text-right" id="step3"><span
                                                                        id="three">delivered</span></li>
                                                                {{else}}
                                                                {{#ifCond this.orderDetails.status "shipped"}}
                                                                <li class="step0 active" id="step1">Order placed</li>
                                                                <li class="step0 active text-center" id="step2">shipped
                                                                </li>
                                                                <li class="step0  text-right" id="step3"><span
                                                                        id="three">delivered</span></li>
                                                                {{else}}
                                                                {{#ifCond this.orderDetails.status "delivered"}}
                                                                <li class="step0 active" id="step1">Order placed</li>
                                                                <li class="step0 active text-center" id="step2">shipped
                                                                </li>
                                                                <li class="step0 active text-right" id="step3"><span
                                                                        id="three">delivered</span></li>
                                                                {{else}}
                                                                {{#ifCond this.orderDetails.status "order canceled"}}
                                                                <li class="step0 " id="step1">Order placed</li>
                                                                <li class="step0  text-center" id="step2">shipped</li>
                                                                <li class="step0  text-right" id="step3"><span
                                                                        id="three">delivered</span></li>
                                                                <li class="step0 active text-right" id="step3"><span
                                                                        id="three" class="text-danger">cancelled</span></li>

                                                                {{else}}
                                                                {{#ifCond this.orderDetails.status "return request"}}
                                                                <li class="step0 active" id="step1">Order placed</li>
                                                                <li class="step0 active text-center" id="step2">shipped
                                                                </li>
                                                                <li class="step0 active text-right" id="step3"><span
                                                                        id="three">delivered</span></li>
                                                                <li class="step0 active text-right" id="step4"><span
                                                                        id="three">pending</span></li>
                                                                {{/ifCond}}
                                                                {{/ifCond}}
                                                                {{/ifCond}}
                                                                {{/ifCond}}
                                                                {{/ifCond}}
                                                                {{!-- {{/each}} return requst pending for validation--}}
                                                            </ul>
                                                        </div>
                                                       

                                                        <div class="modal-footer">
                                                            <button type="button" class="btn btn-secondary"
                                                                data-dismiss="modal">Close</button>
                                                            {{#ifCond this.orderDetails.status "delivered"}}
                                                            <button type="button" class="btn btn-primary"
                                                                data-dismiss="modal"
                                                                onclick="returnProduct('{{this._id}}')">return request
                                                            </button>
                                                            {{/ifCond}}

                                                            {{#ifCond this.orderDetails.status "return accepted"}}

                                                            {{/ifCond}}
                                                            {{#ifCond this.orderDetails.status "placed"}}

                                                            <button type="button" class="btn btn-primary"
                                                                data-dismiss="modal"
                                                                onclick="cancelOrder('{{this._id}}')">cancel
                                                                order</button>
                                                            {{/ifCond}}
                                                            {{#ifCond this.orderDetails.status "shipped"}}

                                                            <button type="button" class="btn btn-primary"
                                                                data-dismiss="modal"
                                                                onclick="cancelOrder('{{this._id}}')">cancel
                                                                order</button>
                                                            {{/ifCond}}
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            {{!--
                                    <td class="product-subtotal text-center"><button type="button"
                                            class="btn btn-small">view
                                            product</button>
                                    </td> --}}
                                </tr>

                                {{/each}}

                            </tbody>
                        </table>
                    </div>
                </form>
            </div>
        </div>
    </div>

</div>

